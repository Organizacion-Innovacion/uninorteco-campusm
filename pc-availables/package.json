  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/styles": "^5.15.20",
    "@ombiel/aek-asset-store": "^0.0.8",
    "@ombiel/aek-css": "1.x.x",
    "@ombiel/aek-lib": ">=1.4.0-0 <1.5.0",
